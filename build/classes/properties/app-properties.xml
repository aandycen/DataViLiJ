        <property name="ALGORITHMS_FOLDER" value="data-vilij/resources/algorithms"/>
        <property name="ALGO_ITERATION_LABEL" value="Iteration: "/>
        <property name="RClusterer" value="RandomClusterer"/>
        <property name="KClusterer" value="KMeansClusterer"/>
