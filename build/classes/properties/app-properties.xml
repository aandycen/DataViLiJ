        <property name="LOAD_DATA_TITLE" value="Load Data File"/>
        <property name="SET_DIRECTORY" value="data-vilij/resources/data"/>
        <property name="NULL" value="null"/>
        <property name="SAVED" value="Save Successful"/>
        <property name="SAVE_IMAGE_EXT" value="png files (*.png)"/>
        <property name="SAVE_IMAGE_PNG" value="*.png"/>
        <property name="PNG" value="png"/>
        
        <!-- Load Data Info -->
        <property name="INFO_ONE" value="Loaded data consists of "/>
        <property name="INFO_TWO" value=" lines. Showing only the first 10 in text area."/>
        
        <property name="RUN_TOOLTIP" value="Run Algorithm"/>
        <property name="RUN_NAME" value="Run"/>
        <property name="CONFIG_BUTTON_CSS_ID" value="algoConfig"/>
        <property name="RADIO_CLASSIFICATION" value="Random Classification"/>
        <property name="RADIO_CLUSTERING" value="Random Clustering"/>
        <property name="K_MEANS_CLUSTERING" value="K-Means Clustering"/>
